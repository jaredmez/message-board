<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <% messages.forEach(message => { %>
      <h3> <%= message.user %> </h3>
      <p> <%= message.text %> </p>
      <p> <%= message.added %> </p>

    <% }); %>
    <!-- <p>Welcome to <%= title %></p> -->
  </body>
</html>
