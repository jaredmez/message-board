<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <a href="/new" class="mainBtn">Whats on your mind?</a>
    <% messages.forEach(message => { %>
      <div class= "msgContainer">
        <h3> <%= message.user %> </h3>
        <p> <%= message.text %> </p>
        <p> <%= message.added %> </p>
      </div>
    <% }); %>
  </body>
</html>
